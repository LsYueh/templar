{
  "name": "templar",
  "version": "0.0.0-alpha3",
  "description": "TWSE Transaction Message Protocol (TMP) converter",
  "main": "templar.js",
  "type": "commonjs",
  "exports": {
    ".": {
      "require": "./templar.js",
      "default": "./templar.js"
    },
    "./package.json": "./package.json"
  },
  "scripts": {
    "lint": "eslint ./lib",
    "test": "tap run --allow-empty-coverage --disable-coverage --timeout=60000",
    "test:coverage": "tap run --show-full-coverage --timeout=60000 --coverage-report=text --coverage-report=lcov"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/LsYueh/templar.git"
  },
  "keywords": [
    "TWSE",
    "TMP"
  ],
  "author": "LsYueh",
  "license": "BSD-2-Clause",
  "devDependencies": {
    "@types/node": "^24.2.1",
    "eslint": "^9.33.0",
    "globals": "^16.3.0",
    "semver": "^7.7.2",
    "tap": "^21.1.0"
  },
  "engines": {
    "node": ">=18"
  },
  "dependencies": {
    "iconv-lite": "^0.7.0"
  }
}
